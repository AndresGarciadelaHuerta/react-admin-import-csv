{
  "name": "react-admin-import-csv",
  "version": "0.0.0",
  "description": "CSV import button for react-admin",
  "repository": "https://github.com/benwinding/react-admin-import-csv",
  "homepage": "https://github.com/benwinding/react-admin-import-csv",
  "author": "Ben Winding",
  "license": "MIT",
  "private": false,
  "files": [
    "*.md",
    "lib",
    "esm",
    "src"
  ],
  "main": "lib/index",
  "module": "esm/index.js",
  "scripts": {
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
    "test": "jest",
    "test:badges": "jest-coverage-badges",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react-admin": "3.x"
  },
  "dependencies": {
    "papaparse": "4.x"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.12.0",
    "@types/papaparse": "^5.0.3",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-redux": "^7.1.7",
    "jest-coverage-badges": "^1.1.2",
    "react": "16.8.6",
    "react-admin": "3.x",
    "react-dom": "16.8.3",
    "react-scripts": "2.1.8",
    "ts-jest": "^25.2.1",
    "typescript": "3.3.4000"
  }
}
